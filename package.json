{
  "name": "web-sample-develop",
  "version": "1.0.0",
  "description": "網站樣板開發",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist && rm -rf .cache",
    "start": "npm run clean && parcel src/index.html --no-source-maps",
    "build": "npm run clean && parcel build src/index.html --no-source-maps --public-url /web-sample-develop/",
    "deploy": "gh-pages -d dist -m \"Auto-generated update\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VisionYi/web-sample-develop.git"
  },
  "author": "Visionyi - Shane Yi, Hong",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-preset-env": "^1.7.0",
    "gh-pages": "^1.2.0",
    "parcel-bundler": "^1.9.7"
  }
}
